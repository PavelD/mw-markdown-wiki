{
    "name": "MarkdownWiki",
    "version": "0.3.0",
    "author": "Pavel Dobes",
    "url": "https://github.com/PavelD/mw-markdown-wik",
    "description": "Uses Markdown for parsing to wiki text",
    "license-name": "MIT",
    "type": "parserhook",
    "requires": {
        "MediaWiki": ">= 1.29.0"
    },
    "AutoloadClasses": {
        "MarkdownWikiExtension": "includes/MarkdownWikiExtension.php"
    },
    "Hooks": {
        "ParserBeforeInternalParse": "MarkdownWikiExtension::onParserBeforeInternalParse",
        "BeforePageDisplay": "MarkdownWikiExtension::onBeforePageDisplay"
    },
    "config": {
        "MarkdownWikiDefaultOn": {
            "value": false
        },
        "MarkdownWikiToggleFormat": {
            "value": "{{%s}}"
        },
        "MarkdownWikiHighlight": {
            "value": false
        },
        "MarkdownWikiHighlightJs": {
            "value": null
        },
        "MarkdownWikiHighlightCss": {
            "value": null
        }
    },
    "load_composer_autoloader": true,
    "manifest_version": 2
}
